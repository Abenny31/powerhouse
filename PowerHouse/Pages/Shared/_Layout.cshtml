                </div>
            </div>
        </div>
    </div>
    <!-- Copyright End -->

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/PestKit-1.0.0/lib/wow/wow.min.js"></script>
    <script src="~/PestKit-1.0.0/lib/easing/easing.min.js"></script>
    <script src="~/PestKit-1.0.0/lib/waypoints/waypoints.min.js"></script>
    <script src="~/PestKit-1.0.0/lib/owlcarousel/owl.carousel.min.js"></script>

    <!-- Template Javascript -->
    <script src="~/PestKit-1.0.0/js/main.js"></script>



    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>



